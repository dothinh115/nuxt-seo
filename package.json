{
  "name": "@enfyra/nuxt-seo",
  "version": "0.1.12",
  "description": "A powerful, configurable SEO module for Nuxt 4 with automatic OG image generation",
  "type": "module",
  "main": "./module.ts",
  "types": "./module.d.ts",
  "files": [
    "module.ts",
    "module.d.ts",
    "index.ts",
    "src/**/*"
  ],
  "keywords": [
    "nuxt",
    "nuxt-module",
    "nuxt4",
    "seo",
    "meta-tags",
    "open-graph",
    "twitter-cards",
    "structured-data",
    "json-ld",
    "sitemap",
    "robots-txt",
    "og-image",
    "pwa",
    "webmanifest"
  ],
  "license": "MIT",
  "author": {
    "email": "dothinh115@gmail.com"
  },
  "exports": {
    ".": {
      "types": "./module.d.ts",
      "import": "./module.ts"
    },
    "./types": {
      "types": "./src/types.ts",
      "import": "./src/types.ts"
    },
    "./src/composables/usePageSEO": {
      "types": "./src/composables/usePageSEO.ts",
      "import": "./src/composables/usePageSEO.ts"
    },
    "./src/composables/useSEO": {
      "types": "./src/composables/useSEO.ts",
      "import": "./src/composables/useSEO.ts"
    },
    "./src/types/nuxt-imports": {
      "types": "./src/types/nuxt-imports.d.ts"
    },
    "./src/server/routes/robots": "./src/server/routes/robots.ts",
    "./src/server/routes/robots.ts": "./src/server/routes/robots.ts",
    "./src/server/routes/sitemap": "./src/server/routes/sitemap.ts",
    "./src/server/routes/sitemap.ts": "./src/server/routes/sitemap.ts",
    "./src/server/routes/webmanifest": "./src/server/routes/webmanifest.ts",
    "./src/server/routes/webmanifest.ts": "./src/server/routes/webmanifest.ts",
    "./src/server/routes/og": "./src/server/routes/og.ts",
    "./src/server/routes/og.ts": "./src/server/routes/og.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/enfyra/nuxt-seo"
  },
